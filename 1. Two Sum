class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int>m;
        vector<int>result;
        for(int i=0;i<nums.size();i++){
            int res = target-nums[i];
            if(m.find(res)!=m.end()){
                result.push_back(m[res]);
                result.push_back(i);
            }
            else{
                m[nums[i]]=i;
            }
        }
        return result;
    }
};
