class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        int j=0; //pointer to place next no zero element initially at 0 because we want to place first non zero element at 0th index
        for(int i=0;i<nums.size();i++){
            if(nums[i]!=0){
                swap(nums[i],nums[j]);
                j++;
            }
        }
    }
};
