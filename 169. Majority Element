class Solution {
public:
    int majorityElement(vector<int>& nums) {
        //moore's voting algorithm
        int cnt=0;
        int candidate=0;
        for(int i=0;i<nums.size();i++){
            if(cnt==0){
                candidate=nums[i];
            }
            if(nums[i]==candidate){
                cnt++;
            }
            else{
                cnt--;
            }
        }
        return candidate;
    }
};


Using Maps


class Solution {
public:
    int majorityElement(vector<int>& nums) {
        unordered_map<int,int>m;
        for(int i=0;i<nums.size();i++){
            m[nums[i]]++;
        }
        for(auto it:m){
            if(it.second>nums.size()/2){
                return it.first;
            }
        }
        return -1;
    }
};
